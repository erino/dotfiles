[alias]
	br = branch
	ca = commit --amend
	ci = commit
	co = checkout
	com = checkout master
	dc = diff --cached
	di = diff
	last = log -1 HEAD
	rc = rebase --continue
	ri = rebase --interactive
	st = status -s
	unstage = reset HEAD --
	x = !open -a GitX .
	cleanup = "!git branch --merged | grep  -v '\\*\\|master\\|develop' | xargs -n 1 git branch -d"
[push]
	default = simple
[branch]
	autosetuprebase = always
[core]
	excludesfile = /Users/erin/.gitignore
[rebase]
	autosquash = true
